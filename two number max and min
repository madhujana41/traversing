#include <stdio.h>
#include <stdlib.h>
int main() {
    int a,b,max,min;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);
    max=((a+b) + abs(a-b))/2;
    min=((a+b) - abs(a-b))/2;
    printf("the maximum values is %d\n",max);
    printf("the minimum values is %d\n",min);
    return 0;
}
