#include <stdio.h>
int main() {
    unsigned int a,b;
    printf("Enter the value of a:");
    scanf("%u", &a);
    printf("Enter the value of b:");
    scanf("%u", &b);
    printf("a = %u , b = %u" ,a,b);
    printf("a&b = %u \n",a & b);
    printf("a|b = %u \n",a | b);
    printf("a^b = %u \n",a ^ b);
    printf("negative a=%u \n", -a);
    printf("a<<1= %u \n",a<<1);
    printf("a>>1= %u \n",a>>1);
    return 0;
}
