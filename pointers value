#include <stdio.h>
int main() {
             int a=10;
             int *ptr = &a;
             printf(" value of a :%d \n",a);
             printf(" address of a :%p \n", &a);
             printf(" pointer stores the address of a :%p \n",ptr);
             printf("values stored at pointer of a:%d\n", *ptr);
             return 0;
             
} 
